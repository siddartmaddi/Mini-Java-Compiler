class a
{
    public static void main(String args[])
    {
        int b = 1;
        boolean a = b == 1 && false;
        if(b != 1)
        {
            int n = 1;
            n = n + 1;
        }
        else
        {
            do
            {
                int m = 2;
                int x = m + 1;
                x = x + 1;
            }
            while(b == 1);
        }
    }                
